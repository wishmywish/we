        <SCRIPT type=text/javascript>
            $(function () {
                var listWrap = $('.vediomainsp').find('li');
                listWrap.mouseenter(function () {
                    var meEl = $(this);
                    meEl.find('.vediomainsptitle').hide();
                    meEl.find('.vediomainspnr').show();
                });
                listWrap.mouseleave(function () {
                    var meEl = $(this);
                    meEl.find('.vediomainsptitle').show();
                    meEl.find('.vediomainspnr').hide();
                });
                //右侧咨询窗 浮层 效果
                var piaoTabEl = $('.piaotab');
                piaoTabEl.find('.piaotabn2').mouseenter(function () {
                    $(this).find('.piaotab1-pic').show();
                }).mouseleave(function () {
                    $(this).find('.piaotab1-pic').hide();
                });
                piaoTabEl.find('.piaotabn1').mouseenter(function () {
                    $(this).find('.piaotab1-pic1').show();
                }).mouseleave(function () {
                    $(this).find('.piaotab1-pic1').hide();
                });

            });

            function show(o, url) {
                var o = document.getElementById(o);
                var spw = document.getElementById("spw");

                spw.innerHTML = '<EMBED id="sp" name="fsVideo" width="700"' +
                        'height="525" align="middle" src="' + url + '" ' +
                        'type="application/x-shockwave-flash" flashvars="auto=1" allowfullscreen="true" ' +
                        'allowscriptaccess="always" quality="high">' +
                        '</EMBED>';
                o.style.display = "";
                $('#inner').trigger('mouseenter');
                $('.pop-layover').show();
                //$('#inner').trigger('mouseenter');
            }

            function hide(o) {
                var o = document.getElementById(o);
                o.style.display = "none";
                //window.location.reload();
                var spw = document.getElementById("spw"),
                        sp = document.getElementById("sp");
                spw.removeChild(sp);
                $('#inner').trigger('mouseleave');
                $('.pop-layover').hide();
            }

        </SCRIPT>


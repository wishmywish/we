<script type="text/javascript">
$(function(){
   var topNavWrap=$('.xiaobaotitle'),
	   xiaobaoAtplEl=topNavWrap.find('a').filter('.xiaobao-atpl');
	var topNavWrap=$('.xiaobaotitle');
	xiaobaoAtplEl.click(function(){

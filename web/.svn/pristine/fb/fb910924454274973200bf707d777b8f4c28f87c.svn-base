<script type="text/javascript">
$(function(){
   var topNavWrap=$('.xiaobaotitle'),
	   fxkAtplEl=topNavWrap.find('a').filter('.fxk-atpl');
	var topNavWrap=$('.xiaobaotitle');
	fxkAtplEl.click(function(){
		fxkAtplEl.removeClass('ton');

            $(document).ready(function () {
                var backtopEl = $('#gotopbtn');
                var winEl = $(window);
                winEl.scroll(function () {
                    var scrollTop = winEl.scrollTop();
                    if (scrollTop > 0) {
                        backtopEl.show();
                    } else {
                        backtopEl.hide();
                    }
                });
                backtopEl.click(function () {
                    $('html,body').animate({scrollTop: '0px'}, 500);
                });
                $('.banner-c-min').bind('click', function () {
                    slideLarge();
                });
                var slideSmall = function () {
                    $('.banner-c').animate({
                        height: 100
                    }, {
                        easing: 'easeInQuad',
                        duration: 1000,
                        step: function () {
                            var pos = $('.enter-demo-link').offset();
                            $('.mnty').parent().css('top', pos.top + 58 - 224);
                        },
                        complete: function () {
                            $('.banner-c').hide();
                            $('.banner-c-min').show();
                        }
                    });
                };
                var slideLarge = function () {
                    $('.banner-c-min').hide();
                    $('.banner-c').css('height', 100).show();
                    $('.banner-c').animate({
                        height: 700
                    }, {
                        easing: 'easeInQuad',
                        duration: 1000,
                        step: function () {
                            var pos = $('.enter-demo-link').offset();
                            $('.mnty').parent().css('top', pos.top + 58 - 224);
                        },
                        complete: function () {
                            $('.banner-c').show();
                            $('.banner-c-min').hide();
                            setTimeout(function () {
                                slideSmall();
                            }, 10000);
                        }
                    });
                };
            });
